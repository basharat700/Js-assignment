    let FirstSubject = prompt("Enter your First Subject Name");
    let SecondSubject = prompt("Enter your Second Subject Name");
    let ThirdSubject = prompt("Enter your Third Subject Name");

    let FirstSubjectObtainedMarks = +prompt(`Enter ${FirstSubject} Obtaine Marks.`);
    let SecondSubjectObtainedMarks = +prompt(`Enter ${SecondSubject} Obtaine Marks.`);
    let ThirdSubjectObtainedMarks = +prompt(`Enter ${ThirdSubject} Obtaine Marks.`);
    
    let totalMArks = 100;
    
    let TotalObtainedMarks = FirstSubjectObtainedMarks+SecondSubjectObtainedMarks+ThirdSubjectObtainedMarks;
    let TotalPercentage = (TotalObtainedMarks / 300 ) * 100;
    document.write(`<table>
        <thead>
            <tr style="font-weight:600; font-size: 20px;">
                <td>Subject </td>
                <td>Total Marks </td>
                <td>Marks Obtained </td>
                <td>Percentage </td>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>${FirstSubject}</td>
                <td>${totalMArks}</td>
                <td>${FirstSubjectObtainedMarks}</td>
                <td>${(FirstSubjectObtainedMarks / totalMArks) * 100}%</td>
            </tr>
            <tr>
                <td>${SecondSubject}</td>
                <td>${totalMArks}</td>
                <td>${SecondSubjectObtainedMarks}</td>
                <td>${(SecondSubjectObtainedMarks / totalMArks) * 100}%</td>
            </tr>
            <tr>
                <td>${ThirdSubject}</td>
                <td>${totalMArks}</td>
                <td>${ThirdSubjectObtainedMarks}</td>
                <td>${(ThirdSubjectObtainedMarks / totalMArks) * 100}%</td>
            </tr>
            <tr>
            </tr>
            <tr>
                <td>Total</td>
                <td>${TotalObtainedMarks}</td>
                <td>300</td>
                <td>${TotalPercentage}%</td>

            </tr>
        </tbody>
        
    </table>`)  ;
</script>
